fun outer() {
    var x = "outside";
    fun inner() {
        print x;
    }

    inner();
}

outer();

fun modifyUpvalue() {
    var name = "Shine";
    print "Hello, " + name;

    fun setName(value) {
        name = value;
    }

    setName("John");
    print "Hello, " + name;
}

modifyUpvalue();
